<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<%
    quantite=null;dateAchat=null;idProduit=null;NomProduit=null;PrixProduit=null;PrixCommande=null;
    commande.forEach((cmd, index) => {
        NomProduit=cmd.NomProduit;
        quantite=cmd.quantite;
        dateAchat=cmd.dateAchat;
        idProduit=cmd.idProduit;
        PrixProduit=cmd.PrixProduit;
        PrixCommande=cmd.PrixCommande;
        
     });  %>
    <table class="table" style="width: 75%; height: 250px;" align="center">
        <thead>
            <tr style="height: 80px;">
                <th scope="col">Commande : </th>
                <th scope="col">
                    <%=dateAchat%>
                </th>

            </tr>
            <tr style="height: 80px;">
                <th scope="col">Nom Produit</th>
                <th scope="col">
                    <%=NomProduit%>
                </th>
            </tr>
            <tr style="height: 80px;">
                <th scope="col">Prix Produit</th>
                <th scope="col">
                    <%=PrixProduit%>
                </th>
            </tr>
            <tr style="height: 80px;">
                <th scope="col">Quantit√© </th>
                <th scope="col">
                    <%=quantite%>
                </th>
            </tr>
            <tr bgcolor="yellow" style="height: 80px;">
                <th scope="col">Prix Commande </th>
                <th scope="col">
                    <%=PrixCommande%>
                </th>
            </tr>
            <tr>
                <td colspan="2" align="right" onclick="window.print()">Imprimer</td>
            </tr>
        </thead>

    </table>